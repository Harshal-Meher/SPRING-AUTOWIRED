<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
               
     <context:annotation-config/>
  
    
    <bean class="autowire.anotation.Address" name="address">
        
        <property name="city" value="Jaypur"/>
        <property name="loaction" value="Shivaji Nager"/>
        
    </bean>
       
    <bean class="autowire.anotation.Address" name="address1">
        
        <property name="city" value="Pune"/>
        <property name="loaction" value="Ram Nager"/>
        
    </bean>
    
    <bean class="autowire.anotation.Emp" name="emp1"/>
    
    
    
    
    </beans>